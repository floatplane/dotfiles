;;
;; Configuration for chemacs (https://github.com/plexus/chemacs2)
;;
;; .emacs.d contains chemacs package, user emacs directories are loaded from the settings below
;;
(
 ("default" . ((user-emacs-directory . "~/dotfiles/emacs.d")))
 ("doom" . ((user-emacs-directory . "~/doom-emacs")
            (env . (("DOOMDIR" . "~/dotfiles/doom.d"))))))
 )
